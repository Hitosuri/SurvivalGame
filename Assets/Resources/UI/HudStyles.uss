    white-space: nowrap;
    text-overflow: clip;
    font-size: 7px;
    background-image: url('project://database/Assets/Texture2D/UI/Hud/Status/QuestUI_CountFrame%20(2).png?fileID=2800000&guid=d77bf079f77ddad4ebedcb333184f78c&type=3#QuestUI_CountFrame (2)');
    max-width: none;
    width: 50px;
    height: 15px;
    align-items: stretch;
    justify-content: flex-start;
    align-self: center;
    flex-direction: column;
    position: relative;
    -unity-background-image-tint-color: rgba(60, 60, 60, 0.65);
    -unity-background-scale-mode: scale-to-fit;
