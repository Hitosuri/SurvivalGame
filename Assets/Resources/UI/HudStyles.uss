.IconInfo {
    top: 30px;
    -unity-text-align: upper-center;
    -unity-font-definition: url('project://database/Assets/Font/NotoSansCJKkr-DemiLight%20SDF.asset?fileID=11400000&guid=887f571ca93f58e488be54a634a7da9a&type=2#NotoSansCJKkr-DemiLight SDF');
